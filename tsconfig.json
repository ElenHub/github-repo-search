{
  "compilerOptions": {
    "target": "esnext",
    "module": "ESNext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node", 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}